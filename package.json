{
  "name": "@fidiora/lightweight-cookie-consent",
  "version": "1.0.0",
  "description": "A lightweight, robust, and customizable cookie consent solution for GDPR and CCPA compliance, maintained by Fidiora",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build": "babel src -d dist",
    "prepare": "husky install"
  },
  "keywords": [
    "consent-banner",
    "gdpr",
    "ccpa",
    "privacy",
    "cookies",
    "consent-management",
    "fidiora",
    "cybersecurity",
    "identity"
  ],
  "author": "Fidiora (https://fidiora.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fidiora/lightweight-cookie-consent.git"
  },
  "bugs": {
    "url": "https://github.com/Fidiora/lightweight-cookie-consent/issues"
  },
  "homepage": "https://fidiora.com",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "sri-toolbox": "^0.2.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "nodemon": "^3.0.2",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
